<workbook>
    <worksheet name="Прошедшие аусвайсы">
        <row>
            <format bold="on" font="Arial" size="12">
                <cell><var name="event_name" /></cell>
            </format>
        </row>
        <row>
            <format bold="on" font="Arial" size="10">
                <cell>Прошедшие аусвайсы</cell>
            </format>
        </row>
        <row></row>
    
        <format border="1">
            <row>
                <cell width="15">Время</cell>
                <cell width="25">Команда</cell>
                <cell width="20">Ник</cell>
                <cell width="30">ФИО</cell>
            </row>
            <loop name="list">
                <format font="Arial">
                <row>
                    <format align="right">
                    <cell type="string" text="$event_dtadd_format" />
                    </format>
                    <cell type="string" text="$command_name" />
                    <cell type="string" text="$nick" />
                    <cell type="string" text="$fio" />
                </row>
                </format>
            </loop>
        </format>
        
    </worksheet>
</workbook>
