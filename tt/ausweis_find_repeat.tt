
<table class="tbl">
[% IF find %]
    <tr class="title">
        <td width="200">Команда</td>
        <td width="200">Ник</td>
        <td>ФИО</td>
    </tr>
[% IF list_nick.size > 0 %]
    <tr class="title"><td colspan="3">Повторы <u>в Никах</u></td></tr>
    [% FOREACH group = list_nick %]
        [% FOREACH group %]
